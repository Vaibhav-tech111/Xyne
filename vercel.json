{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@GEMINI_API_KEY",
    "GROQ_API_KEY": "@GROQ_API_KEY",
    "HF_API_KEY": "@HF_API_KEY",
    "REDIS_URL": "@REDIS_URL",
    "ENV": "production"
  }
}
